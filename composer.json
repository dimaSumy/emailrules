  "type": "magento-module",
  "minimum-stability": "dev",
  "license": "MIT",
  "description": "Magento project with deployment script (composer.json) to create Magento Composer.",
  "authors": [
    {
      "name": "Aws Team",
      "email": "team@aws3.net"
    }
  ],
  "require": {
    "magento-hackathon/magento-composer-installer": "3.0.*",
    "magento-hackathon/composer-command-integrator": "*",
    "aydin-hassan/magento-core-composer-installer" : "~1.0",
    "magento/core": "^1.9.3",
    "madalinoprea/magneto-debug":"^1.8.1"
  },

  "suggest":{
        "n98/magerun":"*",
  	    "madalinoprea/magneto-debug":"^1.8.1",
        "aoepeople/aoe_templatehints": "v0.4.1",
        "aschroder/smtp_pro": "v2.0.5",
        "magento-hackathon/hackathon_magemonitoring": "^2.1.4",
         "avstudnitz/scopehint": "^0.6",
    "tim-reynolds/magento-qconfig": "1.0",
    "aoepeople/aoe_scheduler": "^1",
    "aschroder/smtp_pro": "^2",
    "gordonlesti/lesti_fpc": "^1",
    "jeroenvermeulen/solarium": "v1.6.9-beta",
    "absolutewebservices/magento-1.x-ext-a-aws-magento-advanced-code-editor":"dev-master",
    "absolutewebservices/magento-1.x-ext-a-aws-migration-cms-content":"dev-master",
    "absolutewebservices/magento-1.x-ext-a-aws-frontendguide":"^1.0.0"
   },

  "repositories": [
    {
      "type": "composer",
      "url": "http://packages.firegento.com"
    },
    {
      "type": "composer",
      "url": "http://composer.stores.aws3.net/packages.json"
    }
  ],

  "extra": {
    "magento-root-dir": "./",
    "magento-deploystrategy": "symlink",
    "magento-force": true,
    "auto-append-gitignore": true,
