        "cron.php",
        "favicon.ico",
        "/lib",
        "/js",
        "skin/install/default",
        "skin/adminhtml/default",
        "skin/frontend/default",
        "skin/frontend/base",
        "skin/frontend/rwd",
        "/errors",
        "media/xmlconnect",
        "media/dhl",
        "media/downloadable",
        "media/customer",
        "app/code/core/Mage",
        "app/code/core/Zend",
        "app/code/core/Enterprise",
